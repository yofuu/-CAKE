<h2>商品新規登録</h2>


<%= form_with model: item do |f| %>

　<h4>商品画像</h4>
 <%= f.file_field :image %>

  <h4>商品名</h4>
  <%= f.text_field :name %>

 <h4>商品説明</h4>
 <%= f.text_area :introduction %>

 <h4>ジャンル</h4>
 <%= f.text_field :gener_id %>

 <h4>税抜価格</h4>
 <%= f.text_field :prise %> "円"

 <h4>販売ステータス</h4>
 <%= f.text_field :is_active %>

 <%= f.submit '新規登録' %>
<% end %>